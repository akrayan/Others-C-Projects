int	my_putchar(char c)
{
  write(1, &c , 1);
}

int	my_aff_alpha()
{
  int i ;

  i = 97 ;
  while( i <= 122 )
    {
      my_putchar(i);
      i = i+1 ;
    }
}

int	main()
{
  my_aff_alpha();
}
